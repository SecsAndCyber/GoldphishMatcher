<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android">
	
	<application android:label="GoldphishMatcher" android:debuggable="false" android:icon="@drawable/icon">
		
		<activity android:name="MainActivity" android:exported="true" android:launchMode="singleTask" android:label="GoldphishMatcher" android:configChanges="keyboard|keyboardHidden|orientation" android:screenOrientation="sensorPortrait">
			
			<intent-filter>
				
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
				<category android:name="tv.ouya.intent.category.GAME" />
				
			</intent-filter>
			
		</activity>
		
	</application>
	
</manifest>
